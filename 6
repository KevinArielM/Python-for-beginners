edad_1= float(input("Ingresa edad_1: "))
edad_2=float(input("Ingresa edad_2: "))
if edad_1>edad_2:
  print("La edad1 es mayor que edad2")
elif edad_2>edad_1:
    print("La edad2 es mayor que edad1")
else:
  print("La edad2 es mayor que la edad1")

